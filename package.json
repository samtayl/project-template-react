{
  "name": "project-template-node",
  "version": "0.0.0",
  "type": "commonjs",
  "description": "A template for my react projects",
  "keywords": [
    "template",
    "react"
  ],
  "license": "0BSD",
  "author": "samtayl",
  "homepage": "https://github.com/samtayl/project-template-react#readme",
  "bugs": "https://github.com/samtayl/project-template-react/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/samtayl/project-template-react"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "develop": "cross-env NODE_ENV=development webpack serve",
    "lint:code": "eslint -c eslint.config.js --ext .js .",
    "lint:code:fix": "yarn lint:code --fix",
    "lint:other": "prettier --check \"**/*.{json,md,yml}\"",
    "lint:other:fix": "yarn lint:other --write",
    "lint:spelling": "cspell \"**\"",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint",
    "postinstall": "husky install"
  },
  "files": [
    "src/**/*"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@samtayl/eslint-config": "^1.2.0",
    "@samtayl/eslint-config-node": "^1.1.0",
    "@svgr/webpack": "^6.2.1",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "cspell": "^6.1.2",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.18.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.2.1"
}
